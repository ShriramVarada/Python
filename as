[32m+[m[41m[m
[32m+[m[32mdef insertionsort(arr):[m[41m[m
[32m+[m[32m    comparisons = 0[m[41m[m
[32m+[m[32m    for i in range(1, len(arr)):[m[41m[m
[32m+[m[32m        v = arr[i][m[41m[m
[32m+[m[32m        j = i - 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # Since we make 2 comparisons in the while loop, we increment comparisons by 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while j >= 0 and arr[j] > v:[m[41m[m
[32m+[m[32m            comparisons += 2[m[41m[m
[32m+[m[32m            arr[j + 1] = arr[j][m[41m[m
[32m+[m[32m            j -= 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        comparisons += 1[m[41m[m
[32m+[m[32m        arr[j + 1] = v[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    return comparisons[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("")[m[41m[m
[32m+[m[32mprint("Insertion sort running times in terms of comparisons made: ")[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("N = 50: " + str(insertionsort(L1I)))[m[41m[m
[32m+[m[32mprint("N = 100: " + str(insertionsort(L2I)))[m[41m[m
[32m+[m[32mprint("N = 1000: " + str(insertionsort(L3I)))[m[41m[m
[32m+[m[32mprint("N = 10000: " + str(insertionsort(L4I)))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef partition(arr, l, r):[m[41m[m
[32m+[m[32m    global comparisonforQuick[m[41m[m
[32m+[m[32m    s = l  # index of smaller element[m[41m[m
[32m+[m[32m    pivot = arr[l]  # pivot[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    for j in range(l + 1, r+1):[m[41m[m
[32m+[m[32m        comparisonforQuick += 1[m[41m[m
[32m+[m[32m        if arr[j] < pivot:[m[41m[m
[32m+[m[32m            s = s + 1[m[41m[m
[32m+[m[32m            arr[s], arr[j] = arr[j], arr[s][m[41m[m
[32m+[m[41m[m
[32m+[m[32m    arr[s], arr[l] = arr[l], arr[s][m[41m[m
[32m+[m[32m    return s[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef quicksort(arr, l, r):[m[41m[m
[32m+[m[32m    global comparisonforQuick[m[41m[m
[32m+[m[32m    comparisonforQuick += 1[m[41m[m
[32m+[m[32m    if l < r:[m[41m[m
[32m+[m[32m        s = partition(arr, l, r)[m[41m[m
[32m+[m[32m        quicksort(arr, l, s - 1)[m[41m[m
[32m+[m[32m        quicksort(arr, s+1, r)[m[41m[m
[32m+[m[32m    return comparisonforQuick[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("")[m[41m[m
[32m+[m[32mprint("Quicksort running times in terms of comparisons made: ")[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforQuick = 0[m[41m[m
[32m+[m[32mprint("N = 50: " + str(quicksort(L1Q, 0, len(L1Q) - 1)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforQuick = 0[m[41m[m
[32m+[m[32mprint("N = 100: " + str(quicksort(L2Q, 0, len(L2Q) - 1)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforQuick = 0[m[41m[m
[32m+[m[32mprint("N = 1000: " + str(quicksort(L3Q, 0, len(L3Q) - 1)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforQuick = 0[m[41m[m
[32m+[m[32mprint("N = 10000: " + str(quicksort(L4Q, 0, len(L4Q) - 1)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef mergeSort(alist):[m[41m[m
[32m+[m[32m    global comparisonforMerge[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    comparisonforMerge += 1[m[41m[m
[32m+[m[32m    if len(alist) > 1:[m[41m[m
[32m+[m[32m        mid = len(alist)//2[m[41m[m
[32m+[m[32m        lefthalf = alist[:mid][m[41m[m
[32m+[m[32m        righthalf = alist[mid:][m[41m[m
[32m+[m[41m[m
[32m+[m[32m        mergeSort(lefthalf)[m[41m[m
[32m+[m[32m        mergeSort(righthalf)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        i = j = k = 0[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while i < len(lefthalf) and j < len(righthalf):[m[41m[m
[32m+[m[32m            comparisonforMerge += 1[m[41m[m
[32m+[m[32m            if lefthalf[i] < righthalf[j]:[m[41m[m
[32m+[m[32m                alist[k] = lefthalf[i][m[41m[m
[32m+[m[32m                i = i+1[m[41m[m
[32m+[m[32m            else:[m[41m[m
[32m+[m[32m                alist[k] = righthalf[j][m[41m[m
[32m+[m[32m                j = j+1[m[41m[m
[32m+[m[32m            k = k+1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while i < len(lefthalf):[m[41m[m
[32m+[m[32m            comparisonforMerge += 1[m[41m[m
[32m+[m[32m            alist[k] = lefthalf[i][m[41m[m
[32m+[m[32m            i = i+1[m[41m[m
[32m+[m[32m            k = k+1[m[41m[m
[32m+[m[32m        comparisonforMerge += 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while j < len(righthalf):[m[41m[m
[32m+[m[32m            comparisonforMerge += 1[m[41m[m
[32m+[m[32m            alist[k] = righthalf[j][m[41m[m
[32m+[m[32m            j = j+1[m[41m[m
[32m+[m[32m            k = k+1[m[41m[m
[32m+[m[32m        comparisonforMerge += 1[m[41m[m
[32m+[m[32m    return comparisonforMerge[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("")[m[41m[m
[32m+[m[32mprint("Mergesort running times in terms of comparisons made: ")[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforMerge = 0[m[41m[m
[32m+[m[32mprint("N = 50: " + str(mergeSort(L1M)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforMerge = 0[m[41m[m
[32m+[m[32mprint("N = 100: " + str(mergeSort(L2M)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforMerge = 0[m[41m[m
[32m+[m[32mprint("N = 1000: " + str(mergeSort(L3M)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforMerge = 0[m[41m[m
[32m+[m[32mprint("N = 10000: " + str(mergeSort(L4M)))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef heapify(arr, n, i):[m[41m[m
[32m+[m[32m    global comparisonforHeap[m[41m[m
[32m+[m[32m    largest = i[m[41m[m
[32m+[m[32m    l = 2*i+1[m[41m[m
[32m+[m[32m    r = 2*i+2[m[41m[m
[32m+[m[32m    comparisonforHeap+=2[m[41m[m
[32m+[m[32m    if l < n and arr[i] < arr[l]:[m[41m[m
[32m+[m[32m        largest = l[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    comparisonforHeap += 1 # On average[m[41m[m
[32m+[m[32m    if r < n and arr[largest] < arr[r]:[m[41m[m
[32m+[m[32m        largest = r[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if largest != i:[m[41m[m
[32m+[m[32m        arr[i],arr[largest] = arr[largest],arr[i][m[41m[m
[32m+[m[41m[m
[32m+[m[32m        heapify(arr, n, largest)[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef heapsort(arr):[m[41m[m
[32m+[m[32m    global comparisonforHeap[m[41m[m
[32m+[m[32m    n = len(arr)[m[41m[m
[32m+[m[41m	[m
[32m+[m[32m    for i in range(n, -1, -1):[m[41m[m
[32m+[m[32m        heapify(arr, n, i)[m[41m[m
[32m+[m[32m    for i in range(n-1, 0, -1):[m[41m[m
[32m+[m[32m        arr[i], arr[0] = arr[0], arr[i][m[41m[m
[32m+[m[32m        heapify(arr, i, 0)[m[41m[m
[32m+[m[41m		[m
[32m+[m[32m    return comparisonforHeap[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("")[m[41m[m
[32m+[m[32mprint("Heapsort running times in terms of comparisons made: ")[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforHeap = 0[m[41m[m
[32m+[m[32mprint("N = 50: " + str(heapsort(L1H)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforHeap = 0[m[41m[m
[32m+[m[32mprint("N = 100: " + str(heapsort(L2H)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforHeap = 0[m[41m[m
[32m+[m[32mprint("N = 1000: " + str(heapsort(L3H)))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcomparisonforHeap = 0[m[41m[m
[32m+[m[32mprint("N = 10000: " + str(heapsort(L4H)))[m[41m[m
