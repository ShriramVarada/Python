[32m+[m[32mimport collections[m[41m[m
[32m+[m[41m[m
[32m+[m[32m"""[m[41m[m
[32m+[m[32mProblem 1, Homework 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32mRun this program as main.[m[41m[m
[32m+[m[32mUses doctest to validate the answer as a dictionary which maps the layer number[m[41m[m
[32m+[m[32mto the nodes that are present in it.[m[41m[m
[32m+[m[32mPass to the bfs function, the graph in the form of a dictionary and a starting node[m[41m[m
[32m+[m[32m"""[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef bfs(G, s):[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    """[m[41m[m
[32m+[m[32m    >>> G = {1: [2, 3], 2: [1, 3, 5, 4], 3: [1, 2, 5, 7, 8], 4: [2, 5], 5: [2, 3, 4, 6], 6: {5}, 7: {3, 8}, 8: {3, 7}}[m[41m[m
[32m+[m[32m    >>> print(bfs(G, 1))[m[41m[m
[32m+[m[32m    {1: [1], 2: [2, 3], 3: [5, 4, 7, 8], 4: [6]}[m[41m[m
[32m+[m[32m    """[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    q = collections.deque()[m[41m[m
[32m+[m[32m    q.append(s)[m[41m[m
[32m+[m[32m    discovered = {}[m[41m[m
[32m+[m[32m    parent = {}[m[41m[m
[32m+[m[32m    layer = 1[m[41m[m
[32m+[m[32m    layernode = {1: []}[m[41m[m
[32m+[m[32m    for n in G.keys():[m[41m[m
[32m+[m[32m        discovered[n] = False[m[41m[m
[32m+[m[32m        parent[n] = True[m[41m[m
[32m+[m[32m    discovered[s] = True[m[41m[m
[32m+[m[32m    layernode[layer].append(s)[m[41m[m
[32m+[m[32m    layer += 1[m[41m[m
[32m+[m[32m    while q:[m[41m[m
[32m+[m[32m        numberofNodesinLayer = len(q)[m[41m[m
[32m+[m[32m        while numberofNodesinLayer > 0:[m[41m[m
[32m+[m[32m            u = q.popleft()[m[41m[m
[32m+[m[32m            for v in G[u]:[m[41m[m
[32m+[m[32m                if not discovered[v]:[m[41m[m
[32m+[m[32m                    q.append(v)[m[41m[m
[32m+[m[32m                    parent[v] = u[m[41m[m
[32m+[m[32m                    if layer in layernode:[m[41m[m
[32m+[m[32m                        layernode[layer].append(v)[m[41m[m
[32m+[m[32m                    else:[m[41m[m
[32m+[m[32m                        layernode[layer] = [v][m[41m[m
[32m+[m[32m                    discovered[v] = True[m[41m[m
[32m+[m[32m            numberofNodesinLayer -= 1[m[41m[m
[32m+[m[32m        if numberofNodesinLayer == 0:[m[41m[m
[32m+[m[32m            layer += 1[m[41m[m
[32m+[m[32m    return layernode[m[41m[m
