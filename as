[32m+[m[41m[m
[32m+[m[32m            # TODO: Download the image[m[41m[m
[32m+[m[32m            res = requests.get(comicUrl)[m[41m[m
[32m+[m[32m            res.raise_for_status()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            # TODO: Save the image to ./xkcd[m[41m[m
[32m+[m[32m            imageFile = open(os.path.join('xkcd', os.path.basename(comicUrl)), 'wb')[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for chunk in res.iter_content(100000):[m[41m[m
[32m+[m[32m                imageFile.write(chunk)[m[41m[m
[32m+[m[32m            imageFile.close()[m[41m[m
[32m+[m[32m        except requests.exceptions.MissingSchema:[m[41m[m
[32m+[m[32m            pass[m[41m[m
