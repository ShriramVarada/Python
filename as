[32m+[m[32mdef selectionsort(arr):[m[41m[m
[32m+[m[32m    comparisons = 0[m[41m[m
[32m+[m[32m    length = len(arr)[m[41m[m
[32m+[m[32m    for i in range(length):[m[41m[m
[32m+[m[32m        minimum = i[m[41m[m
[32m+[m[32m        for j in range(i+1, length):[m[41m[m
[32m+[m[32m            comparisons += 1[m[41m[m
[32m+[m[32m            if arr[j] < arr[minimum]:[m[41m[m
[32m+[m[32m                minimum = j[m[41m[m
[32m+[m[32m        arr[i], arr[minimum] = arr[minimum], arr[i][m[41m[m
[32m+[m[32m    return comparisons[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mL1I = L1[:][m[41m[m
[32m+[m[32mL2I = L2[:][m[41m[m
[32m+[m[32mL3I = L3[:][m[41m[m
[32m+[m[32mL4I = L4[:][m[41m[m
[32m+[m[41m[m
[32m+[m[32mL1Q = L1I[:][m[41m[m
[32m+[m[32mL2Q = L2I[:][m[41m[m
[32m+[m[32mL3Q = L3I[:][m[41m[m
[32m+[m[32mL4Q = L4I[:][m[41m[m
[32m+[m[41m[m
[32m+[m[32mL1M = L1Q[:][m[41m[m
[32m+[m[32mL2M = L2Q[:][m[41m[m
[32m+[m[32mL3M = L3Q[:][m[41m[m
[32m+[m[32mL4M = L4Q[:][m[41m[m
[32m+[m[41m[m
