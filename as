[32m+[m[32mos.makedirs('xkcd', exist_ok=True)[m[41m[m
[32m+[m[32mwhile not url.endswith('#'):[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # TODO: Download the page[m[41m[m
[32m+[m[32m    print(f'Downloading {url}')[m[41m[m
[32m+[m[32m    res = requests.get(url)[m[41m[m
[32m+[m[32m    res.raise_for_status()[m[41m[m
[32m+[m[32m    soup = bs4.BeautifulSoup(res.text)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # TODO: Find the URL of the comic image[m[41m[m
[32m+[m[32m    comic = soup.select('#comic img')[m[41m[m
[32m+[m[32m    if not comic:[m[41m[m
[32m+[m[32m        print('Coulnt find comic')[m[41m[m
[32m+[m[32m    else:[m[41m[m
[32m+[m[32m        try:[m[41m[m
