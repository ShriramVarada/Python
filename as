[31m-if __name__ == "__main__":[m
[31m-    import doctest[m
[32m+[m[32mimport collections[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m"""[m[41m[m
[32m+[m[32mProblem 3, Homework 2[m[41m[m
[32m+[m[32mRun this file as main.[m[41m[m
[32m+[m[32mThe test cases used are for reference. If you use a different graph, the program[m[41m[m
[32m+[m[32mmay not print the cycle in the order that you wanted it to be.[m[41m [m
[32m+[m[32mPass to the bfs function, the graph in the form of a dictionary and a starting node[m[41m[m
[32m+[m[32m"""[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef bfs(G, s):[m[41m[m
[32m+[m[32m    """[m[41m[m
[32m+[m[32m    >>> G = {'a': ['c'], 'b': ['c'], 'c': ['a', 'b', 'd', 'e'], 'd': ['c', 'e'], 'e': ['c', 'd']}[m[41m[m
[32m+[m[32m    >>> print(bfs(G, 'c'))[m[41m[m
[32m+[m[32m    ['d', 'e', 'c'][m[41m[m
[32m+[m[32m    >>> G = {'a': ['c'], 'b': ['c'], 'c': ['a', 'b', 'd', 'e'], 'd': ['c'], 'e': ['c']}[m[41m[m
[32m+[m[32m    >>> print(bfs(G, 'c'))[m[41m[m
[32m+[m[32m    [][m[41m[m
[32m+[m[32m    """[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    q = collections.deque()[m[41m[m
[32m+[m[32m    q.append(s)[m[41m[m
[32m+[m[32m    discovered = {}[m[41m[m
[32m+[m[32m    parent = {}[m[41m[m
[32m+[m[32m    T = {s: []}[m[41m[m
[32m+[m[32m    for n in G.keys():[m[41m[m
[32m+[m[32m        discovered[n] = False[m[41m[m
[32m+[m[32m        parent[n] = True[m[41m[m
[32m+[m[32m    discovered[s] = True[m[41m[m
[32m+[m[32m    while q:[m[41m[m
[32m+[m[32m            u = q.popleft()[m[41m[m
[32m+[m[32m            for v in G[u]:[m[41m[m
[32m+[m[32m                if not discovered[v]:[m[41m[m
[32m+[m[32m                    q.append(v)[m[41m[m
[32m+[m[32m                    parent[v] = u[m[41m[m
[32m+[m[32m                    if u in T:[m[41m[m
[32m+[m[32m                        T[u].append(v)[m[41m[m
[32m+[m[32m                    else:[m[41m[m
[32m+[m[32m                        T[u] = [v][m[41m[m
[32m+[m[32m                    discovered[v] = True[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    leftedges = dict(G)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    for node, conn in T.items():[m[41m[m
[32m+[m[32m        for x in conn:[m[41m[m
[32m+[m[32m            leftedges[node].remove(x)[m[41m[m
[32m+[m[32m            leftedges[x].remove(node)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    node1 = None[m[41m[m
[32m+[m[32m    node2 = None[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    for node, conn in leftedges.items():[m[41m[m
[32m+[m[32m        if conn:[m[41m[m
[32m+[m[32m            node1 = node[m[41m[m
[32m+[m[32m            node2 = conn[0][m[41m[m
[32m+[m[32m            break[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if not node1 and not node2:[m[41m[m
[32m+[m[32m        return [][m[41m[m
[32m+[m[32m    else:[m[41m[m
[32m+[m[32m        cycle = [node1, node2][m[41m[m
[32m+[m[32m        hasedge = False[m[41m[m
[32m+[m[32m        while parent[node1] != parent[node2]:[m[41m[m
[32m+[m[32m            node1 = parent[node1][m[41m[m
[32m+[m[32m            node2 = parent[node2][m[41m[m
[32m+[m[32m            if node1 in G[node2]:[m[41m[m
[32m+[m[32m                hasedge = True[m[41m[m
[32m+[m[32m                cycle.append(node1)[m[41m[m
[32m+[m[32m                cycle.append(node2)[m[41m[m
[32m+[m[32m                break[m[41m[m
[32m+[m[32m            cycle.append(node1)[m[41m[m
[32m+[m[32m            cycle.append(node2)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if hasedge:[m[41m[m
[32m+[m[32m        return cycle[m[41m[m
[32m+[m[32m    else:[m[41m[m
[32m+[m[32m        cycle.append(parent[node1])[m[41m[m
[32m+[m[32m        return cycle[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif __name__ == "__main__":[m[41m[m
[32m+[m[32m    import doctest[m[41m[m
     doctest.testmod(verbose=True)[m
\ No newline at end of file[m
